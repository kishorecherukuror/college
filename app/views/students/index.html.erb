<table border=2px;>
<tr>
<th>Name</th>
<th>Age</th>
<th>Address</th>
<th>DOB</th>
<th>Gender</th>
</tr>

<% @students.each do |student| %>
<tr>
<td> <%= student.name %> </td>
<td> <%= student.age %> </td>
<td> <%= student.address %> </td>
<td> <%= student.dob %> </td>
<td> <%= student.gender %> </td>
</tr>

<% end %>
</table>